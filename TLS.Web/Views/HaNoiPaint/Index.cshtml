@section Headers{
    <link href="~/css/hanoi-paint.css" rel="stylesheet" />
    <title>Ha Noi Paint - Ego</title>
}
    <div class="hanoi-page max-width relative">
        <section class="header">
            <div class="row">
                <div class="col-lg-5 col-md-12 col-12"></div>
                <div class="col-lg-7 col-md-12 col-12">
                    <h1>Ha Noi paint</h1>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-5 col-md-12 col-12 padding-left">
                    <div class="row">
                        <div class="col-lg-12 col-md-6 col-6">
                            <div class="title">SERVICES</div>
                        </div>
                        <div class="col-lg-12 col-md-6 col-6">
                            <div class="desc">
                                <p>Event organization</p>
                                <p>Event shooting</p>
                                <p>Roadshow</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-7 col-md-12 col-12">
