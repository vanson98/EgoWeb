@using Microsoft.AspNetCore.Mvc.Localization
@using TLS.Web.Models
@inject IViewLocalizer ViewLocalizer

@section Headers{
    <link href="~/css/hanoi-paint2.css" rel="stylesheet" />
    <title>Ha Noi Paint - Ego</title>
}
<div class="hanoi2-page max-width">
    <section class="header">
        <div class="row">
            <div class="col-lg-5 col-md-12 col-12"></div>
            <div class="col-lg-7 col-md-12 col-12">
                <h1>Ha Noi paint</h1>
            </div>
        </div>
        <div class="row">
                <div class="col-lg-5 col-md-12 col-12 padding-left">
                <div class="row">
                    <div class="col-lg-12 col-md-6 col-6">
                        <div class="title">SERVICES</div>
                    </div>
                    <div class="col-lg-12 col-md-6 col-6">
                        <div class="desc">
                            <p>Branding</p>
                            <p>Campain</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-7 col-md-12 col-12">
                <div class="row">
                    <div class="col-lg-12 col-md-6 col-6">
                        <div class="title">DESCRIPTION</div>
                    </div>
                    <div class="col-lg-12 col-md-6 col-6">
                        <div class="desc">
                            <p>
                                    @ViewLocalizer["hnpb1"]
                                
                                <br/>
                                    @ViewLocalizer["hnpb2"]
                               
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section>
        <img src="./EgoImages/hanoi_paint/hanoi2.png" alt="" />
    </section>
        <div class="max-width">
            @await Html.PartialAsync("_PageNavigation",new NavigationModel(){
        PreviousPagePath = "/hanoi-paint-event",
        NextPagePath = "/esa-english"
        })
        </div>
</div>