@section Headers{
    <link href="~/css/fara-spa.css" rel="stylesheet" />
    <title>Fara Spa - Ego</title>
}
<div class="spa-page">
    <section class="header max-width">
        <div class="row">
            <div class="col-lg-5 col-md-12 col-12"></div>
            <div class="col-lg-7 col-md-12 col-12">
                <h1>Fara spa</h1>
            </div>
        </div>
        <div class="row">
                <div class="col-lg-5 col-md-12 col-12 padding-left">
                <div class="row">
                    <div class="col-lg-12 col-md-6 col-6">
                        <div class="title">SERVICES</div>
                    </div>
                    <div class="col-lg-12 col-md-6 col-6">
                        <div class="desc">
                            <p>Branding</p>
                            <p>Campain</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-7 col-md-12 col-12">
