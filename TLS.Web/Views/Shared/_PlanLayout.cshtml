<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    <link href="https://fonts.googleapis.com/css2?family=Darker+Grotesque:wght@300..900&family=Inter:wght@100..900&family=Manrope:wght@200..800&display=swap"
          rel="stylesheet" />
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css"
          integrity="sha512-tS3S5qG0BlhnQROyJXvNjeEM4UpMXHrQfTGmbQ1gKmelCxlSEBUaxhRBj/EFTzpbP4RVSrpEikbmdJobCvhE3g=="
          crossorigin="anonymous"
          referrerpolicy="no-referrer" />
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.min.css"
          integrity="sha512-sMXtMNL1zRzolHYKEujM2AqCLUR9F2C4/05cdbxjjLSRvMQIciEPCQZo++nk7go3BtSuK9kfa/s+a4f4i5pLkw=="
          crossorigin="anonymous"
          referrerpolicy="no-referrer" />
    <link href="~/css/plan-layout.css" rel="stylesheet" />
    @*Keep*@
    <meta name="google-site-verification" content="-97oijCzFGZ1c1R6FCvL3B9eSum-i3UdV6JES7UTwd0" />
   
    @RenderSection("Headers", required: false)
</head>
<body id="kt_body" class="">
    <div class="bg-white">
        <header>
            <nav class="flex items-center justify-between p-6 max-w-[1360px] mx-auto"
                 aria-label="Global">
                <div class="flex lg:flex-1">
                    <a href="/" class="-m-1.5 p-1.5">
                        <span class="sr-only">LogoYour Company</span>
                        <img class="w-auto lg:h-auto h-12"
                             src="~/EgoImages/plan/logoHeader.svg"
                             alt="" />
                    </a>
                </div>
                <div class="flex lg:hidden open-menu">
                    <button type="button"
                            class="-m-2.5 inline-flex items-center justify-center rounded-md p-2.5 text-gray-700">
                        <span class="sr-only">Open main menu</span>
                        <svg class="h-6 w-6"
                             fill="none"
                             viewBox="0 0 24 24"
                             stroke-width="1.5"
                             stroke="currentColor"
                             aria-hidden="true">
                            <path stroke-linecap="round"
                                  stroke-linejoin="round"
                                  d="M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25h16.5" />
                        </svg>
                    </button>
                </div>
                <div class="hidden lg:flex lg:gap-x-12">
                    <a href="/plan#benefit"
                       class="text-xl font-semibold leading-6 primary-color">Lợi ích</a>
                    <a href="/plan#content"
                       class="text-xl font-semibold leading-6 primary-color">Nội dung <br />bản hoạch định</a>
                    <a href="/plan#procedure"
                       class="text-xl font-semibold leading-6 primary-color">
                        Quy trình <br />
                        triển khai
                    </a>
                    <a href="/plan#survey-section"
                       class="items-center rounded-3xl inline-flex bg-indigo-600 px-8 py-1 text-xl font-semibold text-white shadow-sm hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600">Nhận tư vấn</a>
                </div>
            </nav>
            <!-- Mobile menu, show/hide based on menu open state. -->
            <div class="lg:hidden mobile-menu" role="dialog" aria-modal="true">
                <!-- Background backdrop, show/hide based on slide-over state. -->
                <div class="fixed inset-0 z-50"></div>
                <div class="fixed inset-y-0 right-0 z-50 w-full overflow-y-auto bg-white px-6 py-6 sm:max-w-sm sm:ring-1 sm:ring-gray-900/10">
                    <div class="flex items-center justify-between">
                        <a href="/" class="-m-1.5 p-1.5">
                            <span class="sr-only">Your Company</span>
                            <img class="w-auto lg:h-auto h-12"
                                 src="~/EgoImages/plan/logoHeader.svg"
                                 alt="" />
                        </a>
                        <button type="button"
                                class="-m-2.5 rounded-md p-2.5 text-gray-700 close-menu">
                            <span class="sr-only">Close menu</span>
                            <svg class="h-6 w-6"
                                 fill="none"
                                 viewBox="0 0 24 24"
                                 stroke-width="1.5"
                                 stroke="currentColor"
                                 aria-hidden="true">
                                <path stroke-linecap="round"
                                      stroke-linejoin="round"
                                      d="M6 18L18 6M6 6l12 12" />
                            </svg>
                        </button>
                    </div>
                    <div class="mt-6 flow-root">
                        <div class="-my-6 divide-y divide-gray-500/10">
                            <div class="space-y-2 py-6">
                                <a href="/plan#benefit"
                                   class="-mx-3 block rounded-lg px-3 py-2 text-base font-semibold leading-7 primary-color hover:bg-gray-50">Lợi ích</a>
                                <a href="/plan#content"
                                   class="-mx-3 block rounded-lg px-3 py-2 text-base font-semibold leading-7 primary-color hover:bg-gray-50">Nội dung bản hoạch định</a>
                                <a href="/plan#procedure"
                                   class="-mx-3 block rounded-lg px-3 py-2 text-base font-semibold leading-7 primary-color hover:bg-gray-50">Quy trình triển khai</a>
                                <a href="/plan#survey-section"
                                   class="rounded-2xl inline-flex bg-indigo-600 px-8 py-1 text-sm font-semibold text-white shadow-sm hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600">Nhận tư vấn</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
       
        @RenderBody()

        <footer class="bg-gradient-to-r from-[#3D64EF] to-[#071854]">
            <div class="lg:flex block max-w-[1360px] lg:mx-auto mx-5 py-10">
                <div class="lg:w-[300px]">
                    <div class="place-content-center grid">
                        <a href="/"><img src="~/EgoImages/plan/logoFooter.svg" alt="" /></a>
                    </div>
                    <div class="text-white -mt-4 text-center">
                        One choice for full value
                    </div>
                </div>
                <div class="grid grid-cols-12 gap-3 text-white darker-grotesque-font items-start py-10">
                    <div class="lg:col-span-2 lg:block hidden"></div>
                    <div class="lg:col-span-2 col-span-6">
                        <div class="font-[800] text-md">Email</div>
                        <div class="font-bold text-[13px]">hello@egocreative.co</div>
                    </div>
                    <div class="lg:col-span-2 lg:block hidden">
                        <div class="font-[800] text-md">Website</div>
                        <div class="font-bold text-[13px]">egocreative.co</div>
                    </div>
                    <div class="lg:col-span-2 col-span-6">
                        <div class="font-[800] text-md">Điện thoại</div>
                        <div class="font-bold text-[13px]">0876 280 678</div>
                    </div>
                    <div class="lg:col-span-2 col-span-6">
                        <div class="font-[800] text-md">Địa chỉ</div>
                        <div class="font-bold text-[13px]">
                            Tầng 4, Số 5, Ngõ 897 Giải Phóng, Hoàng Mai, Hanoi, Vietnam
                        </div>
                    </div>
                    <div class="flex items-center lg:col-span-2 col-span-6 gap-3">
                        <a href="https://www.facebook.com/egoagency.vn">
                            <img src="~/EgoImages/plan/facebook.svg" alt="" class="w-12" />
                        </a>
                        <a href="https://www.instagram.com/ego_hellocreative/">
                            <img src="~/EgoImages/plan/ins.svg" alt="" class="w-12" />
                        </a>
                        <a href="https://www.youtube.com/@@easygo.creative">
                            <img src="~/EgoImages/plan/youtube.svg" alt="" class="w-12" />
                        </a>
                    </div>
                </div>
            </div>
        </footer>
    </div>

   
    


    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js"></script>
    <script src="https://cdn.tailwindcss.com"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js"
            integrity="sha512-v2CJ7UaYy4JwqLDIrZUI/4hqeoQieOmAZNXBeQyjo21dadnwR+8ZaIJVT8EE2iyI61OV8e6M8PP2/4hpQINQ/g=="
            crossorigin="anonymous"
            referrerpolicy="no-referrer"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"
            integrity="sha512-bPs7Ae6pVvhOSiIcyUClR7/q2OAsRiovw4vAkX+zJbw3ShAeeqezq50RIIcIURq7Oa20rW2n2q+fyXBNcU9lrw=="
            crossorigin="anonymous"
            referrerpolicy="no-referrer"></script>

    
    <script src="~/js/site/plan-layout.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>